      <Label layoutX="84.0" layoutY="26.0" text="True/False question" />
      <Label fx:id="lblQuestion" layoutX="84.0" layoutY="78.0" text="Label" />
      <RadioButton fx:id="rbTrue" layoutX="84.0" layoutY="129.0" mnemonicParsing="false" text="True" toggleGroup="$tgAnswers" />
      <RadioButton fx:id="rbFalse" layoutX="84.0" layoutY="179.0" mnemonicParsing="false" text="False" toggleGroup="$tgAnswers" />
      <Button fx:id="btnPrevious" layoutX="87.0" layoutY="245.0" mnemonicParsing="false" text="Previous Question" />
      <Button fx:id="btnNext" layoutX="249.0" layoutY="245.0" mnemonicParsing="false" onAction="#handleNextButtonAction" text="Next Question" />
