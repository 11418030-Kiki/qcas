<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qcas.MCQuestionController">
   <children>
      <Label layoutX="63.0" layoutY="30.0" text="Multiple Choice Question (Single correct Answer)" />
      <Label fx:id="lblQuestion" layoutX="63.0" layoutY="78.0" text="Label" />
      <RadioButton fx:id="rbA" layoutX="63.0" layoutY="129.0" mnemonicParsing="false" text="RadioButton">
         <toggleGroup>
            <ToggleGroup fx:id="tgAnswers" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="rbB" layoutX="63.0" layoutY="179.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$tgAnswers" />
      <RadioButton fx:id="rbC" layoutX="63.0" layoutY="229.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$tgAnswers" />
      <RadioButton fx:id="rbD" layoutX="63.0" layoutY="279.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$tgAnswers" />
      <Button fx:id="btnNext" layoutX="282.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleNextButtonAction" text="Next Question" />
      <Button fx:id="btnPrevious" layoutX="123.0" layoutY="338.0" mnemonicParsing="false" onAction="#handlePreviousButtonAction" text="Previous Question" />
      <Button fx:id="btnEndTest" layoutX="418.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleEndTestButtonAction" text="End Test" />
   </children>
</AnchorPane>
